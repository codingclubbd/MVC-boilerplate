  <div class="register_wrapper">
    <div class="register_wrap mx-auto">
      <h1 class="register_title">Register Here</h1>

      <!-- register form -->
      <form action="/register" method="post">

        <div class="d-flex justify-content-between">
        <!-- first name section -->

        <div class="mb-2 me-2">
            <label for="firstName" 
              class="form-label text-white">
            <b>First Name </b>
            </label>

            <input  
              class="form-control" 
              value="<%=user.firstName? user.firstName : ''%>" id="firstName"
              type="text"
              placeholder="First Name"
              name="firstName"
              required
            />
            <% if(error.firstName){ %>
              <p class="error"><%= error.firstName.msg %></p>
            <% } %>
              
            <!-- <p class="error">*First name is required</p> -->

          </div>
        <!-- last name section -->

          <div class="mb-2">

            <label for="lastName " class="form-label text-white"><b>Last Name </b></label>

            <input
              class="form-control"
              id="lastName" 
              type="text"
              value="<%=user.lastName? user.lastName : ''%>"
              placeholder="Last Name" 
              name="lastName"
              required
              />
              <% if(error.lastName){ %>
                <p class="error"><%= error.lastName.msg %></p>
              <% } %>
            <!-- <p class="error">*Last name is required</p> -->
          </div>
        </div>


        <!-- user name section -->
       <div class=" mb-2">
          <label for="username" 
          class="form-label text-white">
          <b>Username </b>
          </label>

          <div class="input-group">
          <span 
          class="input-group-text fw-bold text-secondary material-icons" 
          id="basic-addon1">person
          </span>
            <input class="form-control"
            id="userName" type="text"
            placeholder="Enter Your Username"
            name="username"
            value="<%=user.username? user.username : ''%>"
            required
            />
            
          </div>
            <% if(error.username){ %>
              <p class="error"><%= error.username.msg %></p>
            <% } %>
            <p class="error" id="usernameError"></p>
          <!-- <p class="error">*Username is required</p> -->
        </div>


        <!-- email section -->
         <div class="mb-2">
          <label for="email" class="form-label text-white"><b>Email </b></label>

         <div class="input-group">
          <span 
           class="input-group-text fw-bold text-secondary material-icons" 
           id="basic-addon1">email
          </span>
          <input class="form-control"
          id="email" type="email"
           placeholder="Enter Your Email"
           name="email" 
           name="email"
           value="<%=user.email? user.email : ''%>"
           required
            />

         </div>
            <% if(error.email){ %>
              <p class="error"><%= error.email.msg %></p>
            <% } %>
            <p class="error" id="emailError"></p>
          <!-- <p class="error">*Email is required</p> -->
        </div>
        <!--  password section -->

        <div class=" mb-2">
          <label for="firstName" class="form-label text-white"><b>Password </b></label>
          <div class="input-group">
            <span style="cursor:pointer"
            class="input-group-text fw-bold text-secondary material-icons"
            id="basic-addon1">lock</span>

            <input class="form-control passwordFiled"
            id="password"
            type="password"
            placeholder="Enter your password"
            name="password"
            required />

            <span style="cursor:pointer"
            class="passwordToggler input-group-text fw-bold text-secondary material-icons"
            id="basic-addon1">visibility</span>
          </div>

          <!-- <p class="error \">*Password is required</p> -->
          <% if(error.password){ %>
            <p class="error"><%= error.password.msg %></p>
            <% } %>
            <p class="error" id="passwordError"></p>
        </div>


        <!-- confirm password section -->
        <div class=" mb-2">
          <label for="firstName" 
            class="form-label text-white">
           <b>Confirm Password </b>
         </label>
         <div class="input-group">
            <span style="cursor:pointer"
            class="input-group-text fw-bold text-secondary 
            material-icons"
            id="basic-addon1">lock
            </span>

            <input class="form-control confirmPasswordFiled"
            id="confirm_password"
            type="password"
            placeholder="Enter Your Confirm Password"
            name="confirmPassword" 
            required />

            <span style="cursor:pointer"
            class="input-group-text fw-bold text-secondary 
            material-icons confirmPasswordToggler"
            id="basic-addon1">visibility</span>
            
          </div>
            <% if(error.confirmPassword){ %>
            <p class="error"><%= error.confirmPassword.msg %></p>
            <% } %>
            <p class="error" id="confirmPasswordError"></p>
          <!-- <p class="error firstName-error">*Confirm Password is required</p> -->
        </div>
        
        <!-- //check box section -->
        <div class="d-flex login_footer my-4">
          <div class="mb-1 form-check">
           <input 
              type="checkbox"
              class="form-check-input"
              id="exampleCheck1"
              required />

            <label class="form-check-label" 
              for="exampleCheck1">I agree with the terms
            </label>
          </div>
          <!-- register button  -->
          <button type="submit" 
                  id="register" 
                  class="register">
                  Register Now
          </button>
        </div>
      </form>
        <p class="text-light text-center mt-1 mb-2">
          Already have an account? <a href="/login"> Login here </a>
